local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

local Window = redzlib:MakeWindow({
  Title = "Miee Hub : Fish It",
  SubTitle = "By Miee",
  SaveFolder = "Miee hub"
})

Window:AddMinimizeButton({
    Button = { Image = "rbxassetid://134791075681963", BackgroundTransparency = 0 },
    Corner = { CornerRadius = UDim.new(35, 1) },
})

local Tab1 = Window:MakeTab({"Farm", "Home"})
local Section = Tab1:AddSection({"Use Fishing Rod First Before Using Auto Farm"})

-- Variabel untuk menyimpan status dan thread
local AutoFishing = false
local FishingThread = nil

local function StartFishing(perfectCast)
    FishingThread = task.spawn(function()
        while AutoFishing do
            pcall(function()
                -- Langkah 1: Mulai memancing
                local args1 = {
                    1759519253.159527
                }
                game:GetService("ReplicatedStorage")
                    :WaitForChild("Packages"):WaitForChild("_Index")
                    :WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
                    :WaitForChild("RF/ChargeFishingRod"):InvokeServer(unpack(args1))
                
                wait(1)

                -- Langkah 2: Lempar umpan dengan koordinat berbeda
                local args2
                if perfectCast then
                    args2 = {
                        -0.5718746185302734,  -- Koordinat untuk perfect cast
                        0.9785474457785279
                    }
                else
                    args2 = {
                        -1.233184814453125,   -- Koordinat untuk GG cast
                        0.9342951434868079
                    }
                end
                
                game:GetService("ReplicatedStorage")
                    :WaitForChild("Packages"):WaitForChild("_Index")
                    :WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
                    :WaitForChild("RF/RequestFishingMinigameStarted"):InvokeServer(unpack(args2))

                -- Tunggu sinyal ikan menggigit
                wait(3) -- Sesuaikan dengan waktu delay gigitan ikan
                
                -- Langkah 3: Menangkap ikan
                game:GetService("ReplicatedStorage")
                    :WaitForChild("Packages"):WaitForChild("_Index")
                    :WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
                    :WaitForChild("RE/FishingCompleted"):FireServer()
                
                wait(2) -- Delay sebelum ulangi
            end)
        end
    end)
end

local function StopFishing()
    if FishingThread then
        task.cancel(FishingThread)
        FishingThread = nil
    end
end

-- Toggle 1: Auto Farm Normal
Tab1:AddToggle({
    Name = "Auto Farm Awesome Cast",
    Description = "If you experience a bug then turn off the Auto Farm feature and <font color='rgb(255, 0, 0)'>Reset</font> your character.",
    Default = false,
    Callback = function(Value)
        AutoFishing = Value
        if AutoFishing then
            StartFishing(false) -- false untuk normal cast
        else
            StopFishing()
        end
    end
})

local Toggle1 = Tab1:AddToggle({
  Name = "Auto Farm Perfect Cast",
  Description = "If you experience a bug then turn off the Auto Farm feature and <font color='rgb(255, 0, 0)'>Reset</font> your character.",
  Default = false 
})
Toggle1:Callback(function(Value)
 AutoFishing = Value
        if AutoFishing then
            StartFishing(true) -- true untuk perfect cast
        else
            StopFishing()
        end
end)


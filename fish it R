game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net"):WaitForChild("RF/SellAllItems"):InvokeServer()
end})

local Tab1 = Window:MakeTab({"Player", "user"})
local Section = Tab1:AddSection({"Player Menu"})

local InfiniteJump = false

local Toggle1 = Tab1:AddToggle({
  Name = "Infinite Jump",
  Description = "<font color='rgb(255, 0, 0)'>Use at your own risk</font>",
  Default = false 
})
Toggle1:Callback(function(Value)
InfiniteJump = Value
if InfiniteJump then
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local humanoid

-- Fungsi utama infinite jump
local function onJumpRequest()
	if humanoid then
		humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	end
end

-- Saat karakter spawn
local function onCharacterAdded(character)
	humanoid = character:WaitForChild("Humanoid")
	UserInputService.JumpRequest:Connect(onJumpRequest)
	print("[Infinite Jump] Aktif ✅")
end

-- Jalankan langsung jika karakter sudah ada
if player.Character then
	onCharacterAdded(player.Character)
end

player.CharacterAdded:Connect(onCharacterAdded)
else
--// Infinite Jump OFF
-- Tempatkan di StarterPlayer > StarterPlayerScripts (LocalScript)

local UserInputService = game:GetService("UserInputService")

-- Hapus semua koneksi JumpRequest
for _, conn in pairs(getconnections(UserInputService.JumpRequest)) do
	conn:Disconnect()
end

print("[Infinite Jump] Dinonaktifkan ❌")
end
end)

local flyEnabled = false
local flySpeed = 16
local bodyGyro, bodyVelocity
local touchControls = {}
local moveDirection = Vector3.new(0, 0, 0)

-- Fly Function
local function toggleFly(state)
    flyEnabled = state
    
    if flyEnabled then
        -- Enable fly
        local speaker = game:GetService("Players").LocalPlayer
        local chr = speaker.Character
        if not chr then return end
        
        -- Tunggu hingga humanoid tersedia
        local hum
        repeat
            hum = chr:FindFirstChildWhichIsA("Humanoid")
            wait()
        until hum
        
        -- Disable animations
        if chr:FindFirstChild("Animate") then
            chr.Animate.Disabled = true
        end
        
        -- Set platform stand
        hum.PlatformStand = true
        
        -- Create movement parts
        local movePart = chr:FindFirstChild("HumanoidRootPart")
        if not movePart then return end
        
        -- Clean up previous instances
        if bodyGyro then bodyGyro:Destroy() end
        if bodyVelocity then bodyVelocity:Destroy() end
        
        -- Create new instances
        bodyGyro = Instance.new("BodyGyro")
        bodyGyro.P = 9e4
        bodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
        bodyGyro.cframe = movePart.CFrame
        bodyGyro.Parent = movePart
        
        bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.velocity = Vector3.new(0, 0, 0)
        bodyVelocity.maxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyVelocity.Parent = movePart
        
        -- Create Mobile Controls
        local gui = Instance.new("ScreenGui")
        gui.Name = "FlyControls"
        gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
        
        -- Joystick for movement
        local joystickFrame = Instance.new("Frame")
        joystickFrame.Name = "Joystick"
        joystickFrame.Size = UDim2.new(0, 150, 0, 150)
        joystickFrame.Position = UDim2.new(0, 50, 0.7, 0)
        joystickFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        joystickFrame.BackgroundTransparency = 0.5
        joystickFrame.BorderSizePixel = 0
        joystickFrame.Parent = gui
        
        local joystick = Instance.new("Frame")
        joystick.Name = "JoystickButton"
        joystick.Size = UDim2.new(0, 50, 0, 50)
        joystick.Position = UDim2.new(0.5, -25, 0.5, -25)
        joystick.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        joystick.BackgroundTransparency = 0.3
        joystick.BorderSizePixel = 0
        joystick.Parent = joystickFrame
        
        -- Up/Down buttons
        local upButton = Instance.new("TextButton")
        upButton.Name = "UpButton"
        upButton.Size = UDim2.new(0, 80, 0, 80)
        upButton.Position = UDim2.new(0.8, 0, 0.5, -100)
        upButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        upButton.BackgroundTransparency = 0.3
        upButton.Text = "↑"
        upButton.TextSize = 30
        upButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        upButton.Parent = gui
        
        local downButton = Instance.new("TextButton")
        downButton.Name = "DownButton"
        downButton.Size = UDim2.new(0, 80, 0, 80)
        downButton.Position = UDim2.new(0.8, 0, 0.5, 20)
        downButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        downButton.BackgroundTransparency = 0.3
        downButton.Text = "↓"
        downButton.TextSize = 30
        downButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        downButton.Parent = gui
        
        -- Store controls
        touchControls.gui = gui
        touchControls.joystickFrame = joystickFrame
        touchControls.joystick = joystick
        touchControls.upButton = upButton
        touchControls.downButton = downButton
        
        -- Joystick movement
        local joystickActive = false
        local joystickPosition = Vector2.new(joystickFrame.AbsolutePosition.X + 75, joystickFrame.AbsolutePosition.Y + 75)
        
        local function updateMovement(touchPos)
            local relativePos = Vector2.new(touchPos.X - joystickPosition.X, touchPos.Y - joystickPosition.Y)
            local distance = math.min(75, math.sqrt(relativePos.X^2 + relativePos.Y^2))
            local direction = relativePos.Unit
            
            -- Update joystick position
            joystick.Position = UDim2.new(0, direction.X * distance + 50, 0, direction.Y * distance + 50)
            
            -- Set movement direction (invert Y for forward/backward)
            moveDirection = Vector3.new(direction.X, 0, direction.Y)
        end
        
        joystickFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                joystickActive = true
            end
        end)
        
        joystickFrame.InputChanged:Connect(function(input)
            if joystickActive and input.UserInputType == Enum.UserInputType.Touch then
                updateMovement(input.Position)
            end
        end)
        
        joystickFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                joystickActive = false
                joystick.Position = UDim2.new(0.5, -25, 0.5, -25)
                moveDirection = Vector3.new(0, 0, 0)
            end
        end)
        
        -- Up/Down buttons
        local verticalSpeed = 0
        
        upButton.MouseButton1Down:Connect(function()
            verticalSpeed = flySpeed
        end)
        
        upButton.MouseButton1Up:Connect(function()
            verticalSpeed = 0
        end)
        
        downButton.MouseButton1Down:Connect(function()
            verticalSpeed = -flySpeed
        end)
        
        downButton.MouseButton1Up:Connect(function()
            verticalSpeed = 0
        end)
        
        -- Movement loop
        local movementConnection
        movementConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not flyEnabled or not chr or not hum or not hum.Parent then
                movementConnection:Disconnect()
                return
            end
            
            if bodyVelocity then
                local camera = workspace.CurrentCamera
                local cf = camera.CFrame
                
                -- Calculate movement based on camera direction
                local moveVector = cf:VectorToWorldSpace(moveDirection * flySpeed)
                moveVector = Vector3.new(moveVector.X, verticalSpeed, moveVector.Z)
                
                bodyVelocity.Velocity = moveVector
                
                -- Update gyro to face camera direction
                if bodyGyro then
                    bodyGyro.CFrame = cf
                end
            end
        end)
        
        print("Fly enabled - Mobile controls activated")
        
    else
        -- Disable fly
        local speaker = game:GetService("Players").LocalPlayer
        local chr = speaker.Character
        if chr then
            local hum = chr:FindFirstChildWhichIsA("Humanoid")
            if hum then
                hum.PlatformStand = false
                
                if chr:FindFirstChild("Animate") then
                    chr.Animate.Disabled = false
                end
            end
        end
        
        -- Clean up controls
        if touchControls.gui then
            touchControls.gui:Destroy()
            touchControls = {}
        end
        
        -- Clean up physics
        if bodyGyro then
            bodyGyro:Destroy()
            bodyGyro = nil
        end
        
        if bodyVelocity then
            bodyVelocity:Destroy()
            bodyVelocity = nil
        end
        
        moveDirection = Vector3.new(0, 0, 0)
        
        print("Fly disabled")
    end
end

-- Add Fly Toggle
local Toggle1 = Tab1:AddToggle({
  Name = "Fly Enabled",
  Description = "Sorry if it's bad, because I'm already having a headache with this problem",
  Default = false 
})

Toggle1:Callback(function(Value)
    toggleFly(Value)
end)

-- Add Speed Slider
Tab1:AddSlider({
  Name = "Fly Speed",
  Min = 1,
  Max = 300,
  Increase = 1,
  Default = 1,
  Callback = function(Value)
    flySpeed = Value
    print("Fly speed set to: " .. flySpeed)
  end
})

local speedHackEnabled = false
local jumpPowerEnabled = false
local currentSpeed = 16
local currentJumpPower = 50

-- Function untuk mengatur speed
local function setSpeed(value)
    if speedHackEnabled then
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = value
            end
        end
    end
end

-- Function untuk mengatur jump power
local function setJumpPower(value)
    if jumpPowerEnabled then
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = value
            end
        end
    end
end

-- Toggle untuk Speed Hack
local SpeedToggle = Tab1:AddToggle({
  Name = "Speed Hack",
  Description = "<font color='rgb(255, 0, 0)'>Use at your own risk</font>",
  Default = false 
})
SpeedToggle:Callback(function(Value)
    speedHackEnabled = Value
    local player = game.Players.LocalPlayer
    
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if Value then
                -- Aktifkan speed hack
                humanoid.WalkSpeed = currentSpeed
            else
                -- Matikan speed hack (kembali ke normal)
                humanoid.WalkSpeed = 16
            end
        end
    end
end)

-- Slider untuk mengatur kecepatan
Tab1:AddSlider({
  Name = "Speed Value",
  Min = 1,
  Max = 300,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
      currentSpeed = Value
      setSpeed(Value)
  end
})

-- Toggle untuk Jump Power
local JumpToggle = Tab1:AddToggle({
  Name = "Jump Power",
  Description = "<font color='rgb(255, 0, 0)'>Use at your own risk</font>",
  Default = false 
})
JumpToggle:Callback(function(Value)
    jumpPowerEnabled = Value
    local player = game.Players.LocalPlayer
    
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if Value then
                -- Aktifkan high jump
                humanoid.JumpPower = currentJumpPower
            else
                -- Matikan high jump (kembali ke normal)
                humanoid.JumpPower = 50
            end
        end
    end
end)

-- Slider untuk mengatur jump power
Tab1:AddSlider({
  Name = "Jump Power Value",
  Min = 1,
  Max = 300,
  Increase = 1,
  Default = 50,
  Callback = function(Value)
      currentJumpPower = Value
      setJumpPower(Value)
  end
})

-- Auto apply ketika karakter respawn
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    wait(1) -- Tunggu karakter load sempurna
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        if speedHackEnabled then
            humanoid.WalkSpeed = currentSpeed
        else
            humanoid.WalkSpeed = 16
        end
        
        if jumpPowerEnabled then
            humanoid.JumpPower = currentJumpPower
        else
            humanoid.JumpPower = 50
        end
    end
end)

-- Terapkan pengaturan awal jika karakter sudah ada
local function initializeCharacter()
    local player = game.Players.LocalPlayer
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if speedHackEnabled then
                humanoid.WalkSpeed = currentSpeed
            end
            if jumpPowerEnabled then
                humanoid.JumpPower = currentJumpPower
            end
        end
    end
end

-- Jalankan inisialisasi
initializeCharacter()


features:AddButton("TPTOOL",function()
  local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

local tool = Instance.new("Tool")
tool.Name = "Teleport Tool"
tool.RequiresHandle = false
tool.Parent = player.Backpack

tool.Activated:Connect(function()
    local character = player.Character
    if character and mouse.Target then
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            local targetPosition = mouse.Hit.p
            humanoidRootPart.CFrame = CFrame.new(targetPosition)
        end
    end
end)
end)

features:AddButton("TWEENTOOL",function()
  local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local isTeleporting = false

local tool = Instance.new("Tool")
tool.Name = "Teleport Tool"
tool.RequiresHandle = false
tool.Parent = player.Backpack

tool.Activated:Connect(function()
    if isTeleporting then return end -- Mencegah spam klik saat sedang teleport
    
    local character = player.Character
    if character and mouse.Target then
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            isTeleporting = true
            
            local targetPosition = mouse.Hit.p
            local startCFrame = humanoidRootPart.CFrame
            local targetCFrame = CFrame.new(targetPosition)
            local duration = 0.2 -- Durasi teleportasi dalam detik, bisa diubah
            local startTime = tick()
            
            local connection
            connection = game:GetService("RunService").RenderStepped:Connect(function()
                local elapsedTime = tick() - startTime
                local alpha = elapsedTime / duration
                
                if alpha >= 1 then
                    humanoidRootPart.CFrame = targetCFrame
                    isTeleporting = false
                    connection:Disconnect()
                    return
                end
                
                humanoidRootPart.CFrame = startCFrame:Lerp(targetCFrame, alpha)
            end)
        end
    end
end)
end)
